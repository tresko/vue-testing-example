{
  "name": "vue-testing-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "is-ci \"test:unit:coverage\" \"test:unit:watch\"",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(vue|js|json|ts|tsx|jsx)\"",
    "format": "yarn prettier -- --write",
    "check-format": "yarn prettier -- --list-different",
    "tsc": "tsc --noEmit",
    "validate": "npm-run-all --parallel tsc check-format build"
  },
  "dependencies": {
    "@vue/composition-api": "^0.3.4",
    "axios": "^0.19.0",
    "core-js": "^3.4.3",
    "postcss-preset-env": "^6.7.0",
    "register-service-worker": "^1.6.2",
    "tailwindcss": "^1.1.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.1.2",
    "@testing-library/vue": "^4.1.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-e2e-cypress": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "is-ci-cli": "^2.0.0",
    "jest-date-mock": "^1.0.7",
    "jest-runner-eslint": "^0.7.5",
    "jest-watch-select-projects": "^1.0.0",
    "lint-staged": "^9.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "~3.7.3",
    "vue-template-compiler": "^2.6.10"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "gitHooks": {
    "pre-commit": "yarn tsc && lint-staged && yarn build"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
